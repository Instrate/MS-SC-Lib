{
  "env": {
	"browser": true,
	"es2021": true
  },
  "extends": [
	"eslint:recommended",
	"plugin:@typescript-eslint/recommended",
	"plugin:prettier/recommended",
	"prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
	"ecmaVersion": "latest",
	"sourceType": "module"
  },
  "plugins": [
	"@typescript-eslint",
	"prettier"
  ],
  "rules": {
	"prettier/prettier": [
	  "error"
	],
	"linebreak-style": [
	  "error",
	  "unix"
	],
	"quotes": [
	  "error",
	  "double"
	],
	"semi": [
	  "error",
	  "always"
	],
	"no-extra-semi": [
	  "error"
	],
	"no-useless-escape": "off"
  }
}
